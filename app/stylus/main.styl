

/* variables */
$lightgrey = #999
$linkColor = #555
$grey = #eee
$black = #222
$white = #f7f7f7

html
button
input
select
textarea 
  color: $black

*
  margin 0
  padding 0
  -webkit-font-smoothing antialiased

a
a:visited
  color: $linkColor
  text-decoration: none

a:hover
  color: lighten($linkColor,10%)
  
body
  background #fff
  color: $black
  font-size: 1em
  line-height: 1.5
  -webkit-user-select none
  user-select none
  -webkit-tap-highlight-color transparent
  -webkit-overflow-scrolling touch
  font-family: '.SFNSDisplay-Ultralight','Helvetica Neue','Helvetica','STHeitiSC-Light','Arial',sans-serif;

.test
  background: rgba(54, 150, 146, 0.81);
  width: 100%;
  height: 100vh;
  color: #fff;
  font-size: 23px;
  text-align: center;
  padding-top: 30%;
  font-family: sans-serif;
.header
  position: fixed;
  top: 0;
  width: 100%;
  .header1
    text-align: center;
    font-size 20px
    line-height 40px
    background: #49afbf;
    a
      color: #fff;
      font-weight: 900;
      line-height: 37px;
    span
      position: absolute;
      color: #fff;
      font-weight: 900;
      display: inline-block;
      padding: 5px 15px;
      left: 0;
      top:-4px
  .header2
      background: rgb(228, 228, 230);
      line-height: 36px;
      display: flex;
      justify-content: space-between;
      padding: 0 10px;
      font-size: 13px;
      a
        width: 33px;
        text-align: center;
        &.active
          border-bottom: 2px solid #3c7d9c;
      
.cslist
  margin-top 76px
  padding 0 10px
  .item
    display block
    height 60px
    padding-top 10px 
    padding-bottom: 8px
    border-bottom: 1px solid #eee;
    img
      width 35px
      height 35px
      float left
      padding 5px
  .listp
    margin-left 50px
    p:first-child
      font-size 14px
      font-weight 600
    p:last-child
      font-size 12px
      color #9e9e9e
  .more
    text-align: center;
    line-height: 50px;
    background: #ffffff;
    color: #a5a2a2;


